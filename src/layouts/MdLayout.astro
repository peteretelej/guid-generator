---
import NavBar from "../components/NavBar.astro";
import Layout from "./Layout.astro";

const { title, frontmatter } = Astro.props;
---

<Layout title={frontmatter ? frontmatter.title : title}>
  <NavBar />
  <article class="prose dark:prose-invert px-3 max-w-none">
    <slot />
  </article>
</Layout>
